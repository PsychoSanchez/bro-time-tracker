{
  "name": "bro-time-tracker",
  "version": "0.1.0",
  "description": "Browser time tracking extension",
  "private": true,
  "scripts": {
    "test": "jest",
    "copy-static": "cp -a ./static/. ./dist",
    "build": "npm run copy-static & cross-env NODE_ENV=production node ./esbuild.config.js",
